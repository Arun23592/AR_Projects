package selenium;

public class Test1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String test = "Sunday";
		int iDay = 7;
 
		if(test.equals("Sunday")){
			System.out.println("Today is Sunday");
		}
 
		if(iDay==7){
			System.out.println("Today is Sunday");
		}
	}
}
