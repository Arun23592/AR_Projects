package stringbuffer;

public class Swappingstring {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s1 ="I AM INDIAN";
		String s2 = "INDIAN AM I";
		System.out.println("Before swapping values are : " + s1 +"  "+s2);
		String t=s1;
		s1 = s2;
		s2 = t;
		System.out.println("After swapping values are : " + s1 +"  "+s2);
	}

}
